{
  "identifier": "org.worldtakeoverindustries.ForkLauncher",
  "name": "ForkLauncher",
  "organization": "WorldTakeoverInduatries",
  "description": "Open the current workspace with the Fork Git client",
  "version": "1.0",
  "categories": ["commands"],
  "repository": "https://github.com/JustChapman/ForkLauncher",
  "bugs": "https://github.com/JustChapman/ForkLauncher/issues",
  "license": "MIT",
  "main": "main.js",
  "entitlements": {
    "process": true
  },
  "commands": {
    "extensions": [
      {
        "title": "Open Workspace in Fork",
        "command": "ForkLauncher.runFork",
        "shortcut": "cmd-option-ctrl-f"
      }
    ]
  }
}
